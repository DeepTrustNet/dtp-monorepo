# DTN Network Configuration
# Copy this file to .env and fill in your actual values

# Private Keys (replace with your actual private keys)
OWNER_PRIVATE_KEY=your_owner_private_key_here
WORKER_PRIVATE_KEY=your_worker_private_key_here

# Network Configuration
INFURA_PROJECT_ID=your_infura_project_id_here

# IPFS Configuration (Pinata)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# OpenAI Configuration (for dtn-ai sidecars)
YOUR_OPENAI_API_KEY_ENV_VAR_NAME=your_openai_api_key_here

# Optional: Override default values
# NODE_ENV=production
# PORT=3000 